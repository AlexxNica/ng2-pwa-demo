{
    "angularCompilerOptions": {
        "googleClosureOutput": true
    },
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "target": "es6",
        "noImplicitAny": false,
        "sourceMap": false,
        "experimentalDecorators": true,
        "outDir": "built",
        "declaration": true
    },
    "exclude": [
        "vendor",
        "node_modules",
        "built",
        "dist",
        "typings/main.d.ts",
        "typings/main"
    ]
}
